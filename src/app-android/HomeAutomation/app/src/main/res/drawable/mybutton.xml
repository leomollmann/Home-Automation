<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="@color/HighdBtnColor2" />
            <gradient android:angle="-90" android:startColor="@color/HighdBtnColor2" android:endColor="@color/LowBtnColor2"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="@color/HighdBtnColor1" />
            <solid android:color="@color/HighdBtnColor1"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="@color/HighdBtnColor1" />
            <gradient android:angle="-90" android:startColor="@color/HighdBtnColor1" android:endColor="@color/LowBtnColor1" />
        </shape>
    </item>
</selector>